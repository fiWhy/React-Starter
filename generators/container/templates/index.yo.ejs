import * as React from "react";
import { Link } from "react-router-dom";
<% if(route) { %>import { withRouter } from "react-router-dom";<% } %>
import { connect } from "react-redux";
<% if(action) { %>import { <%= action.upperCamel %>Action } from "./actions/<%= action.dashed %>.action";<% } %>

import <%= component.upperCamel %>Presentation from "./presentations/<%= component.dashed %>.presentation";

class <%= component.upperCamel %> extends React.Component<any, any>{
    constructor(props: any) {
        super(props);
    }

    componentDidMount() {
    }
    
    render() {
        return <<%= component.upperCamel %>Presentation/>;
    }
}<% if(reducer) { %>
const mapStateToProps = (state) => {
    return {
        <%= reducer.lower %>: state.<%= component.upperCamel %><%= reducer.upperCamel %>Reducer
    }
}<% } %>
<% if(action && reducer) { %>
const mapDispatchToProps = (dispatch) => {
    return {
        <%= action.upperCamel %>: () => dispatch(<%= action.upperCamel %>Action())
    }
}<% } %>

export default <% if(route) { %>withRouter(<% } %>connect(<% if(action && reducer) { %>mapStateToProps, mapDispatchToProps<% } %>)(<%= component.upperCamel %>)<% if(route) { %>);<% } %>